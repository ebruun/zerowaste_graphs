def nodes_with_only_outgoing_edges(G):
    result_nodes = []
    
    for node in G.nodes():
        if G.in_degree(node) == 0:
            result_nodes.append(node)
    
    return result_nodes

def unroll_sequence(G):

    K = G.copy()

    n = nodes_with_only_outgoing_edges(K)
    print("only outgoing", n)

    print(K.nodes())

    K.remove_nodes_from(n)

    print(K.nodes())

    return K